public class Seven7 {
	public static void main(String[] args) {
		int[] num=new int[10];
		double sum=0;
		for(int i=0;i<10;i++)
		{
			int n=(int)(Math.random()*10+1);
			num[i]=n;
			sum+=n;
		}
		
		System.out.print("랜덤한 정수들:");
		for(int i=0;i<num.length;i++)
		{
			System.out.print(num[i]+" ");
		}
		
		System.out.println("평균은"+sum/10);
	}
}
