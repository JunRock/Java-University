public class Ten10 {
	public static void main(String[] args) {
		int[][]num=new int[4][4];
		int k=0;
		for(int i=0;i<4;i++)
		{
			for(int j=0;j<4;j++)
			{				
				num[i][j]=(int)(Math.random()*11+0);
				if(num[i][j]==0)
					k++;
				if(k>6)
					num[i][j]=(int)(Math.random()*10+1);
						
				System.out.printf("%3d",num[i][j]," ");
			}
			System.out.println();
		}
	}
}
