package 연습;

class Acc{
	public static void main(String[] args)
	{
		int[][] arr=new int[3][4];
		int num=1;
		int i,j;
		for(i=0;i<3;i++)
		{
			for(j=0;j<4;j++)
			{
				arr[i][j]=num;
				num++;
			}
		}
		
		for(i=0;i<3;i++)
		{
			for(j=0;j<4;j++)
			{
				System.out.println(arr[i][j]+"\t");
				System.out.println();
			}
		}
	}
}
