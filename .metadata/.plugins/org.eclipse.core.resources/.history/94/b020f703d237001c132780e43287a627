package 연습;

import java.util.Scanner;
class Acc{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);//키보드입력 클래스
		Scanner sc1=new Scanner(System.in);
		 System.out.print("문자열 입력:");
		 int st1=sc.nextInt();
		 System.out.print("문자열 입력:");
		 String st2=sc1.nextLine();
		 System.out.printf("입력된 문자열:%d\n",st1);
		 System.out.printf("입력된 문자열:%s\n",st2);
	}
}
